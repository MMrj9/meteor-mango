    "@emotion/styled": "^11.10.5",
    "chakra-ui-autocomplete": "^1.4.5",
    "formik": "^2.4.5",
    "framer-motion": "^8.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.22.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "yup": "^1.3.3"
